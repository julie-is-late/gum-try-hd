{
	"manifest_version": 3,
	"name": "HD Camera Feed in Video Calls",
	"author": "Kyle Simpson",
	"version": "1",
	"description": "Try to enforce HD (16:9) camera aspect ratio for web-video calls",
	"icons": {
		"16": "images/logo-16.png",
		"24": "images/logo-24.png",
		"32": "images/logo-32.png",
		"48": "images/logo-48.png",
		"96": "images/logo-96.png",
		"128": "images/logo-128.png"
	},
	"options_ui": {
		"page": "settings.html"
	},
	"background": {
		"service_worker": "js/controller.js"
	},
	"content_scripts": [],
	"web_accessible_resources": [
		{
			"resources": ["js/patch-gum.js"],
			"matches": ["<all_urls>"]
		}
	],
	"permissions": [
		"storage",
		"scripting",
		"tabs"
	],
	"host_permissions": [
		"https://*/*"
	]
}
